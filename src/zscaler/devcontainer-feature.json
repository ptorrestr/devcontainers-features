{
    "name": "Zscaler",
    "id": "zscaler",
    "version": "0.0.1",
    "description": "Add support for Zscaler configuration",
    "options": {
        "certificateLocation": {
            "type": "string",
            "enum": [
                "/usr/local/share/ca-certificates/custom-ca/ZscalerRootCertificate-2048-SHA256.crt"
            ],
            "default": "/usr/local/share/ca-certificates/custom-ca/ZscalerRootCertificate-2048-SHA256.crt",
            "description": "Set the location where to save the certificate file."
        },
        "pythonBinPaths": {
            "type": "string",
            "enum": [
                "",
                "/usr/bin/python3"
            ],
            "default": "",
            "description": "List of python binaries to install the certificate."
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
